<?xml version="1.0"?>
<!-- See http://kapo-cpp.blogspot.com/2008/02/register-your-own-mime-type-on-free.html --> 
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-iannix">
  
    <comment>A score editor based on the UPIC sequencer by Iannis Xenakis</comment>
    
    <sub-class-of type="audio/midi"/>

    <glob pattern="*.ian"/>

  </mime-type>
</mime-info>

